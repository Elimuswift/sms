    "type": "library",
    "require": {
        "guzzlehttp/guzzle" : "~6.0",
        "weezqyd/africastalking":"~1.0"
    },
    "autoload": {
        "psr-4": {
            "Elimuswift\\SMS\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elimuswift\\SMS\\SmsServiceProvider"
            ]
        }
    },
    "minimum-stability": "alpha",
    "license": "MIT",
    "require-dev": {
        "roamtech/gateway-api": "^0.0.7@alpha"
    }
}
